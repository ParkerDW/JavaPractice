package training;
import java.util.*;

public class HelloWorld {
	
	
	public enum Test {
		ONE, TWO, THREE, FOUR
	}
	
	public static String hey = "Hello";
	Test aTest;
	
	public HelloWorld(Test test){
		this.aTest = test;
	}
	
	public static void main(String[] args){
		HelloWorld newWorld = new HelloWorld(Test.ONE);
		int isIt = hey.compareTo("Ho");
		System.out.println(isIt);
		ArrayList<HelloWorld> crapola = new ArrayList<HelloWorld>();
		crapola.add(newWorld);
		System.out.println(crapola.get(0));
	}
